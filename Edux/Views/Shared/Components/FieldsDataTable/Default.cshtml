@model IEnumerable<Edux.Models.Field>
<p>
  <a asp-controller="Fields" asp-action="Create" class="btn btn-primary" asp-route-formýd="@ViewBag.formId">Yeni Alan Ekle</a>
 </p>
<table class="table table-striped table-hover" id="tblRecords">
    <thead>
        <tr>
                <th class="text-nowrap all">
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.DisplayName)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.Tab)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.Row)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.Col)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.Position)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.EditorType)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.DefaultValue)
                </th>
                @*<th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.CreateDate)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.CreatedBy)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.UpdateDate)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.UpdatedBy)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.AppTenantId)
                </th>*@
            <th class="text-nowrap ">
                @Html.DisplayNameFor(model => model.Form)
            </th>
            <th class="text-nowrap ">
                @Html.DisplayNameFor(model => model.Property)
            </th>
            <th class="text-nowrap ">
                @Html.DisplayNameFor(model => model.PropertyValue)
            </th>
            <th data-orderable="false" class="text-nowrap all">Eylemler</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DisplayName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tab)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Row)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Col)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Position)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EditorType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DefaultValue)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AppTenantId)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Form.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Property.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PropertyValue.Id)
            </td>
            <td class="text-nowrap">
                <a asp-action="Edit" asp-controller="Fields" asp-route-id="@item.Id" class="btn btn-primary ">Düzenle</a>
                <a asp-action="Details" asp-controller="Fields" asp-route-id="@item.Id" class="btn btn-success">Detaylar</a>
                <a asp-action="Delete" asp-controller="Fields" asp-route-id="@item.Id" class="btn btn-danger">Sil</a>
            </td>
        </tr>
}
    </tbody>
</table>
