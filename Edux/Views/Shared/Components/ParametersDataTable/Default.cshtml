@model IEnumerable<Edux.Models.Parameter>

  <p>
    <a asp-action="Create" asp-controller="Parameters" class="btn btn-primary" asp-route-componenttypeId="@ViewBag.ComponenttypeId">Yeni Parametre Ekle</a>
</p>

    <table class="table table-striped table-hover" id="tblRecords">
        <thead>
            <tr>
                <th class="text-nowrap all">
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.DisplayName)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.IsRequired)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.Position)
                </th>
                @*<th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.CreateDate)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.CreatedBy)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.UpdateDate)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.UpdatedBy)
                </th>
                <th class="text-nowrap ">
                    @Html.DisplayNameFor(model => model.AppTenantId)
                </th>*@
                <th class="text-nowrap">
                    @Html.DisplayNameFor(model => model.ComponentType)
                </th>
                <th data-orderable="false" class="text-nowrap all">Eylemler</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DisplayName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IsRequired)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Position)
                    </td>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.CreateDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CreatedBy)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UpdateDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UpdatedBy)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.AppTenantId)
                    </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.ComponentType.Name )
                    </td>
                    <td class="text-nowrap">
                        <a asp-action="Edit"  asp-controller="Parameters"  asp-route-id="@item.Id" class="btn btn-primary ">Düzenle</a>
                        <a asp-action="Details"  asp-controller="Parameters"  asp-route-id="@item.Id" class="btn btn-success">Detaylar</a>
                        <a asp-action="Delete"  asp-controller="Parameters"  asp-route-id="@item.Id" class="btn btn-danger">Sil</a>
                    </td>
                </tr>
}
        </tbody>
    </table>



